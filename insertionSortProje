                   [22,27,16,2,18,6] -> Insertion Sort
[22, 27, 16, 2, 18, 6] -> [22] [27, 16, 2, 18, 6]    n
[22] [27, 16, 2, 18, 6] -> [22, 27] [16, 2, 18, 6]  n-1
[22, 27] [16, 2, 18, 6] -> [16, 22, 27] [2, 18, 6]  n-2
[16, 22, 27] [2, 18, 6] -> [2, 16, 22, 27] [18, 6]  n-3
[2, 16, 22, 27] [18, 6] -> [2, 6, 16, 22, 27] [18]  n-4
[2, 6, 16, 22, 27] [18] ->[2, 6, 16, 18, 22, 27]     n-5

big O = n square.

